{
  "git.confirmSync": false,
  "yaml.maxItemsComputed": 9000,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.rulers": [80],
  "editor.tabSize": 4,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.8,
  "[php]": {
    "editor.rulers": [100]
  },
  "vscode-php-cs-fixer.fixOnSave": true,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "redhat.telemetry.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.tokenColorCustomizations": {
    "[Tokyo Night Horizon]": {
      "textMateRules": [
        {
          "scope": "variable",
          "settings": {
            "foreground": "#D4D4D4"
          }
        }
      ]
    },
    "[Hyper Dracula]": {
      "textMateRules": [
        {
          "scope": [
            "support", 
            "variable.language", 
            "entity.name.variable.parameter", 
            "entity.name.type",
            "storage.type.php",
            "entity.other.inherited-class",
            "variable.language punctuation.definition.variable.php",
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[Darcula Theme from IntelliJ]": {
      "textMateRules": [
        {
          "scope": "variable",
          "settings": {
            "foreground": "#D4D4D4"
          }
        },
        {
          "scope": "keyword.other.type.php",
          "settings": {
            "foreground": "#CC7832"
          }
        }
      ]
    },
    "[GitHub Dark Mode]": {
      "textMateRules": [
        {
          "scope": ["entity", "entity.name.variable.parameter"],
          "settings": {
            "foreground": "#C9D1D9"
          }
        },
        {
          "scope": [
            "variable.language.this",
            "punctuation.section.embedded.begin.php"
          ],
          "settings": {
            "foreground": "#FF7B72"
          }
        }
      ]
    },
    "[Red One Dark Pro | Dark]": {
      "textMateRules": [
        {
          "scope": ["source.php", "comment.block.documentation"],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": "entity.other.inherited-class",
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "scope": "variable",
          "settings": {
            "fontStyle": "bold",
            "foreground": "#BBBBBB"
          }
        }
      ]
    },
    "[Red One Dark Pro | Dark Bordered]": {
      "textMateRules": [
        {
          "scope": "source.php",
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": "entity.other.inherited-class",
          "settings": {
            "foreground": "#E5C07B"
          }
        }
      ]
    },
    "[Omni-Customized]": {
      "textMateRules": [
        {
          "scope": [
            "support",
            "variable.language",
            "entity.other.inherited-class",
            "entity.name.type",
            "entity.other.attribute-name",
            "storage.type.php"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[One Monokai Darker]": {
      "textMateRules": [
        {
          "scope": [
            "keyword",
            "support.class",
            "comment",
            "variable.language",
            "variable.parameter"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["source.php", "support.class", "source.c"],
          "settings": {
            "fontStyle": "bold"
          }
        }
      ]
    }
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "git.suggestSmartCommit": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.colorTheme": "Hyper Dracula"
}
