{
  "editor.fontFamily": "JetBrains Mono Thin",
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.5,
  "editor.fontSize": 15,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.rulers": [80],
  "editor.wordWrapColumn": 80,
  "vim.enableNeovim": true,
  "vim.smartRelativeLine": true,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.lineHeight": 1,
  "terminal.integrated.fontFamily": "JetBrains Mono Thin",
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "[rust]": {
    "editor.formatOnSave": false
  },
  "[c]": {
    "editor.wordBasedSuggestions": false,
    "editor.formatOnSave": false,
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.rulers": [84],
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 2
  },
  "editor.tokenColorCustomizations": {
    "[monokai-charcoal (red)]": {
      "textMateRules": [
        {
          "scope": ["variable.parameter.c", "support.type", "storage.type"],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[One Monokai]": {
      "textMateRules": [
        {
          "scope": ["variable.parameter"],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "foreground": "#E06C75",
          }
        },
        {
          "scope": ["variable.other"],
          "settings": {
            "foreground": "#56B6C2",
          }
        }
      ]
    },
    "[Darkbox (Darker)]": {
      "textMateRules": [
        {
          "scope": [
            "storage",
            "keyword",
            "entity.name.type.c",
            "entity.name.function.c",
            "constant.language.c",
            "meta.function-call.c",
            "entity.name.function.preprocessor.c",
            "variable.other.enummember.c",
            "entity.name.function",
            "entity.name.type",
            "support.type",
            "entity.other.attribute-name",
            "constant"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["entity.name.function.c"],
          "settings": {
            "foreground": "#83A598"
          }
        },
        {
          "scope": ["storage.modifier", "storage.type.function"],
          "settings": {
            "foreground": "#FB4934"
          }
        },
        {
          "scope": ["variable.parameter.c", "variable.other.local.c"],
          "settings": {
            "foreground": "#e1e1e6e8",
            "fontStyle": ""
          }
        }
      ]
    },
    "[Dark Ocean]": {
      "textMateRules": [
        {
          "scope": [
            "variable.language punctuation.definition.variable.php",
            "meta.type.parameters entity.name.type",
            "entity.other.inherited-class",
            "entity.other.attribute-name",
            "variable.parameter",
            "variable.language",
            "entity.name.type",
            "support"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": [
            "support",
            "entity.other.inherited-class",
            "entity.name.type",
            "variable.language",
            "variable.language punctuation.definition.variable.php"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": "support.function",
          "settings": {
            "foreground": "#50FA7B"
          }
        },
        {
          "scope": "variable",
          "settings": {
            "foreground": "#E1E1E6"
          }
        }
      ]
    },
    "[Dracula Soft]": {
      "textMateRules": [
        {
          "scope": [
            "support",
            "entity.name.type",
            "variable.language",
            "variable.language punctuation.definition.variable.php",
            "entity.other.inherited-class",
            "storage.type.php"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "support.function",
          "settings": {
            "foreground": "#50FA7B"
          }
        }
      ]
    },
    "[Min Dark]": {
      "textMateRules": [
        {
          "scope": "variable.language.this",
          "settings": {
            "foreground": "#F97583"
          }
        },
        {
          "scope": "meta.function-call",
          "settings": {
            "foreground": "#BBBBBB"
          }
        }
      ]
    }
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    },
    "Ubuntu-20.04 (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu-20.04"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "emmet.excludeLanguages": ["markdown", "php"],
  "diffEditor.ignoreTrimWhitespace": false,
  "redhat.telemetry.enabled": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "yaml.maxItemsComputed": 12000,
  "workbench.editorAssociations": {
    "*.sqlite": "default"
  },
  "workbench.colorCustomizations": {
    "[Min Dark]": {
      "editor.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[Dracula]": {
      "editor.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[Dracula Soft]": {
      "editor.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[One Monokai]": {
      "editor.background": "#000000",
      "terminal.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[OM Theme (No Italic)]": {
      "editor.background": "#000000",
      "terminal.background": "#000000",
      "sideBar.background": "#000000"
    }
  },
  "cSpell.userWords": ["cnpj"],
  "workbench.iconTheme": "helium-icon-theme",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.colorTheme": "One Monokai"
}
