{
  "files.exclude": {
    "**/.git": false,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  // "editor.fontFamily": "JetBrains Mono Thin",
  "editor.fontFamily": "Anonymous Pro",
  "editor.fontLigatures": true,
  "editor.lineHeight": 2,
  "editor.fontWeight": "normal",
  "editor.fontSize": 18,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.rulers": [80],
  "editor.wordWrapColumn": 80,
  "vim.enableNeovim": true,
  "vim.smartRelativeLine": true,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.lineHeight": 1.5,
  // "terminal.integrated.fontFamily": "JetBrains Mono Thin",
  "terminal.integrated.fontFamily": "Anonymous Pro",
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "never",
  "rust-analyzer.inlayHints.chainingHints.enable": true,
  "[rust]": {
    "editor.formatOnSave": false
  },
  "[c]": {
    "editor.wordBasedSuggestions": false,
    "editor.formatOnSave": false,
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.rulers": [84],
    "editor.defaultFormatter": "junstyle.php-cs-fixer",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[python]": {
    "editor.tabSize": 2
  },
  "editor.tokenColorCustomizations": {
    "[Pale Fire Darker]": {
      "textMateRules": [
        {
          "scope": "support.class",
          "settings": {
            "foreground": "#E2D082",
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["entity.name.type", "entity.other.inherited-class.php"],
          "settings": {
            "foreground": "#3FB0B6",
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[Miramare]": {
      "textMateRules": [
        {
          "scope": ["variable.parameter"],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[Abyss]": {
      "textMateRules": [
        {
          "scope": [
            "support.class",
            "storage.type",
            "entity.name.type",
            "entity.other.inherited-class"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": ["storage.type", "storage", "keyword", "variable.language.this.php"],
          "settings": {
            "foreground": "#ff4180",
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["entity.name.type", "entity.other.inherited-class", "entity.name.function"],
          "settings": {
            "foreground": "#FFEEBB",
            "fontStyle": "bold"
          }
        },
        {
          "scope": "support.class",
          "settings": {
            "foreground": "#6688CC",
            "fontStyle": "bold"
          }
        },
        {
          "scope": "variable",
          "settings": {
            "foreground": "#E1E1E6",
          }
        }
      ]
    },
    "[One Monokai]": {
      "textMateRules": [
        {
          "scope": ["variable.parameter"],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "foreground": "#E06C75"
          }
        },
        {
          "scope": [
            "variable.other",
            "entity.name.type",
            "entity.other.inherited-class"
          ],
          "settings": {
            "foreground": "#56B6C2"
          }
        }
      ]
    },
    "[Darkbox (Darker)]": {
      "textMateRules": [
        {
          "scope": [
            "storage",
            "keyword",
            "entity.name.type.c",
            "entity.name.function.c",
            "constant.language.c",
            "meta.function-call.c",
            "entity.name.function.preprocessor.c",
            "variable.other.enummember.c",
            "entity.name.function",
            "entity.name.type",
            "support.type",
            "entity.other.attribute-name",
            "constant"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["entity.name.function.c"],
          "settings": {
            "foreground": "#83A598"
          }
        },
        {
          "scope": ["storage.modifier", "storage.type.function"],
          "settings": {
            "foreground": "#FB4934"
          }
        },
        {
          "scope": ["variable.parameter.c", "variable.other.local.c"],
          "settings": {
            "foreground": "#e1e1e6e8",
            "fontStyle": ""
          }
        }
      ]
    },
    "[Dark Ocean]": {
      "textMateRules": [
        {
          "scope": [
            "variable.language punctuation.definition.variable.php",
            "meta.type.parameters entity.name.type",
            "entity.other.inherited-class",
            "entity.other.attribute-name",
            "variable.parameter",
            "variable.language",
            "entity.name.type",
            "support"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": [
            "support",
            "entity.other.inherited-class",
            "entity.name.type",
            "variable.language",
            "variable.language punctuation.definition.variable.php"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": "support.function",
          "settings": {
            "foreground": "#50FA7B"
          }
        },
        {
          "scope": "variable",
          "settings": {
            "foreground": "#E1E1E6"
          }
        }
      ]
    },
    "[oh-lucy-evening]": {
      "textMateRules": [
        {
          "scope": [
            "comment",
            "comment keyword",
            "storage.type",
            "variable.parameter",
            "entity.other.attribute-name",
            "storage.modifier",
            "variable.language",
            "entity.other.inherited-class",
            "support.type"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "variable.language",
          "settings": {
            "foreground": "#B898DD",
          }
        },
        {
          "scope": ["entity.other.attribute-name"],
          "settings": {
            "foreground": "#7EC49D"
          }
        },
        {
          "scope": ["storage.type", "storage.modifier", "keyword", "keyword.control", "keyword.operator"],
          "settings": {
            "foreground": "#ff4180",
          }
        },
        {
          "scope": ["entity.name.function", "support.function"],
          "settings": {
            "foreground": "#39e084"
          },
        },
        {
          "scope": ["support.class", "entity.other.inherited-class", "entity.name.type.class.php"],
          "settings": {
            "foreground": "#25d1e7",
          }
        },
        {
          "scope": "entity.name",
          "settings": {
            "foreground": "#56C6D5"
          }
        }
      ]
    },
    "[Dracula Soft]": {
      "textMateRules": [
        {
          "scope": [
            "support",
            "entity.name.type",
            "variable.language",
            "variable.language punctuation.definition.variable.php",
            "entity.other.inherited-class",
            "storage.type.php",
            "variable.parameter"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "support.function",
          "settings": {
            "foreground": "#50FA7B"
          }
        }
      ]
    },
    "[Omni Owl (Minimal Italics)]": {
      "textMateRules": [
        {
          "scope": "meta.type.parameters entity.name.type",
          "settings": {
            "foreground": "#988BC7"
          }
        }
      ]
    },
    "[Min Dark]": {
      "textMateRules": [
        {
          "scope": "variable.language.this",
          "settings": {
            "foreground": "#F97583"
          }
        },
        {
          "scope": "meta.function-call",
          "settings": {
            "foreground": "#BBBBBB"
          }
        }
      ]
    }
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    },
    "Ubuntu-20.04 (WSL)": {
      "path": "C:\\WINDOWS\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu-20.04"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "emmet.excludeLanguages": ["markdown", "php"],
  "diffEditor.ignoreTrimWhitespace": false,
  "redhat.telemetry.enabled": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "yaml.maxItemsComputed": 12000,
  "workbench.editorAssociations": {
    "*.sqlite": "default"
  },
  "workbench.colorCustomizations": {
    "[Min Dark]": {
      "editor.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[Dracula]": {
      "editor.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[Dracula Soft]": {
      "editor.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[One Monokai]": {
      "editor.background": "#000000",
      "terminal.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[Theme]": {
      "editor.background": "#000000",
      "terminal.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[OM Theme (No Italic)]": {
      "editor.background": "#000000",
      "terminal.background": "#000000",
      "sideBar.background": "#000000"
    },
    "[oh-lucy-evening]": {
      "editor.background": "#080f11",
      "terminal.background": "#080f11"
    }
  },
  "cSpell.userWords": ["cnpj"],
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "material-icon-theme",
  "intelephense.diagnostics.undefinedProperties": false,
  "workbench.colorTheme": "Abyss",
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar"
}
