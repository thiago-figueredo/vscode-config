    { "key": "ctrl+m", "command": "explorer.newFolder", "when": "explorerViewletFocus" },
    { "key": "ctrl+i", "command": "workbench.action.togglePanel" },
    {
        "key": "ctrl+k",
        "command": "workbench.action.terminal.focus"
    },
    {
