[
  {
    "key": "ctrl+i",
    "command": "composerMode.agent"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.selectTheme"
  },
  {
    "key": "ctrl+shift+h",
    "command": "workbench.action.previousEditor",
    "when": "editorFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+shift+l",
    "command": "workbench.action.nextEditor",
    "when": "editorFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+space",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenNavigatePrevious",
    "when": "inQuickOpen || inFilesPicker || inSearchEditor"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenNavigateNext",
    "when": "inQuickOpen || inFilesPicker || inSearchEditor"
  },
  {
    "key": "ctrl+y",
    "command": "workbench.action.acceptSelectedQuickOpenItem",
    "when": "inQuickOpen || inFilesPicker || inSearchEditor"
  },
  {
    "key": "shift+k",
    "command": "editor.action.showHover",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+shift+t",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+y",
    "command": "workbench.action.acceptSelectedMenuItem",
    "when": "inMenu"
  },
  {
    "key": "ctrl+y",
    "command": "acceptSelectedSuggestion",
    "when": "suggestWidgetVisible && textInputFocus && !inQuickOpen && !inFilesPicker && !inSearchEditor && !inMenu"
  },
  {
    "key": "ctrl+shift+a",
    "command": "composerMode.chat"
  },
  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "ctrl+m",
    "command": "explorer.newFolder",
    "when": "explorerViewletVisible && filesExplorerFocus"
  },
  {
    "key": "ctrl+shift+r",
    "command": "editor.action.goToReferences",
    "when": "editorTextFocus"
  }
]
